{
  "name": "users",
  "plural": "profile",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "name": {
      "type": "string",
      "required": true
    },
    "password": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "accessTokens": {
      "type": "hasMany",
      "model": "accessToken",
      "foreignKey": "userId",
      "options": {
        "disableInclude": true
      }
    }
  },
  "acls": [
 
  ],
  "methods": {
    "login": {
      "accepts": [
        {
          "arg": "userId",
          "type": "string"
        },
        {
          "arg": "userPassword",
          "type": "string"
        }
      ],
      "returns": {
        "arg": "result",
        "type": "object"
      },
      "http": {
        "verb": "post"
      }
    }
  }
}
