{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "mongoDs",
    "public" : true,
    "options": {
      "emailVerificationRequired": true,
      "remoting": {
        "sharedMethods": {
          "*": false,
          "find": true,
          "login": true,
          "logout": true,
          "profile": true,
          "create": true,
          "auth": true,
          "verify": true,
          "prototype.patchAttributes": true,
          "updateAll": true,
          "findById": true,
          "getBranches": true
        }
      }
    }
  },
  "users": {
    "dataSource": "mongoDs",
    "public" : true
  },
  "AccessToken": {
    "dataSource": "mongoDs",
    "public": false,
    "relations": {
      "user": {
        "type": "belongsTo",
        "model": "users",
        "foreignKey": "usersId"
      }
    }
  },
  "ACL": {
    "dataSource": "mongoDs",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongoDs",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mongoDs",
    "public": false
  }
}
